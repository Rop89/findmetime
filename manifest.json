
{
  "manifest_version": 3,
  "name": "Find Me Time",
  "version": "1.0",
  "description": "A simple website that finds you time in your google calendar events or tasks.",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_title": "Find Me Time"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/find-me-time.png",
    "48": "icons/find-me-time.png",
    "128": "icons/find-me-time.png"
  }
}